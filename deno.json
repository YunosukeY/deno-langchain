{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "run": "deno run --deny-env --allow-net --allow-read main.ts",
    "fmt": "deno fmt",
    "fmt:check": "deno fmt --check",
    "lint": "deno lint",
    "lint:fix": "deno lint --fix",
    "check": "deno check **/*.ts",
    "test": "deno test"
  },
  "imports": {
    "dotenv": "https://deno.land/std@0.224.0/dotenv/mod.ts",
    "@langchain/openai": "npm:@langchain/openai@0.0.28"
  }
}
